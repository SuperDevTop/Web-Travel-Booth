    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.9",
    "multer": "^1.4.5-lts.1",
    "node-mailjet": "^6.0.5",
    "nodemailer": "^6.9.14",
    "nodemon": "^2.0.7",
    "socket.io": "^4.1.2"
  }
